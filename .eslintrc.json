{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "google",
      "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "semi": [
          "error",
          "always"
      ],
      "indent": [
          "error",
          4
      ],
      "@typescript-eslint/no-explicit-any": "error"
  }
}
